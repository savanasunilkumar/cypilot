{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 18",
  "extends": "./base.json",
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2022",
    "lib": ["ES2022"],
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "types": ["node"]
  }
}
